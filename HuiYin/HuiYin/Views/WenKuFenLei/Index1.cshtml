@model IEnumerable<HuiYin.Models.WenKuFenLei>

@{
    ViewBag.Title = "Index1";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<p>
    @Html.ActionLink("添加一级分类", "Create1")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FenLeiType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
      
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FenLeiType)
        </td>
        <td>
            @Html.ActionLink("添加子分类", "AddChild", new { id = item.Id })|
            @Html.ActionLink("删除", "Delete", new {id = item.Id})
        </td>
    </tr>
}

</table>
